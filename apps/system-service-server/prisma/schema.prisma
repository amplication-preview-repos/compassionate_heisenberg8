datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model System {
  clientId      String?
  configuration Json?
  createdAt     DateTime @default(now())
  domains       Domain?  @relation(fields: [domainsId], references: [id])
  domainsId     String?  @unique
  id            String   @id @default(cuid())
  name          String?
  updatedAt     DateTime @updatedAt
}

model Domain {
  createdAt  DateTime @default(now())
  domainName String?
  id         String   @id @default(cuid())
  system     System?
  updatedAt  DateTime @updatedAt
}
